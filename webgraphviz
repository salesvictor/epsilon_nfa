digraph regex1 {
	rankdir = "LR";

	0 -> 3 [label="a, b"];
	0 -> 4 [label="b"];
	2 -> 4 [label="b"];
	3 -> 2 [label="a, b"];
	3 -> 3 [label="a, b"];
	3 -> 4 [label="b"];
	4 -> 1 [label="b"];
	4 -> 5 [label="b"];
	4 -> 6 [label="b"];
	5 -> 6 [label="a, b"];
	6 -> 6 [label="a, b"];
	6 -> 1 [label="a, b"];

	1 [shape = doublecircle];
	5 [shape = doublecircle];
	6 [shape = doublecircle];
}


