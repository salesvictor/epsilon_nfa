}


digraph regex4 {
	rankdir = "LR";

	0 -> 3 [label="a"];
	0 -> 5 [label="b"];
	0 -> 6 [label="c"];
	2 -> 5 [label="b"];
	2 -> 6 [label="c"];
	3 -> 1 [label="a"];
	3 -> 2 [label="a"];
	3 -> 3 [label="a"];
	3 -> 4 [label="a"];
	3 -> 5 [label="a, b"];
	3 -> 6 [label="a, c"];
	4 -> 6 [label="c"];
	5 -> 1 [label="b"];
	5 -> 4 [label="b"];
	5 -> 5 [label="b"];
	5 -> 6 [label="b, c"];
	6 -> 1 [label="c"];
	6 -> 6 [label="c"];

	0 [shape=doublecircle];
	1 [shape=doublecircle];
	2 [shape=doublecircle];
	3 [shape=doublecircle];
	4 [shape=doublecircle];
	5 [shape=doublecircle];
	6 [shape=doublecircle];
}